# Base16 Material
# Scheme author: Nate Peterson

[global]
    frame_color = "#eeffff"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#82aaff"
    foreground = "#314549"

[base16_normal]
    msg_urgency = normal
    background = "#263238"
    foreground = "#eeffff"

[base16_critical]
    msg_urgency = critical
    background = "#f07178"
    foreground = "#314549"
