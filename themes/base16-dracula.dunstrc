# Base16 Dracula
# Scheme author: Mike Barkmin (http://github.com/mikebarkmin) based on Dracula Theme (http://github.com/dracula)

[global]
    frame_color = "#e9e9f4"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#62d6e8"
    foreground = "#4d4f68"

[base16_normal]
    msg_urgency = normal
    background = "#282936"
    foreground = "#e9e9f4"

[base16_critical]
    msg_urgency = critical
    background = "#ea51b2"
    foreground = "#4d4f68"
