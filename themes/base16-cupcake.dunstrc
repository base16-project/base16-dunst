# Base16 Cupcake
# Scheme author: Chris Kempson (http://chriskempson.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#8b8198"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7297b9"
    foreground = "#d8d5dd"

[base16_normal]
    msg_urgency = normal
    background = "#fbf1f2"
    foreground = "#8b8198"

[base16_critical]
    msg_urgency = critical
    background = "#d57e85"
    foreground = "#d8d5dd"
