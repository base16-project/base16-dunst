# Base16 OneDark
# Scheme author: Lalit Magant (http://github.com/tilal6991)

[global]
    frame_color = "#abb2bf"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#61afef"
    foreground = "#3e4451"

[base16_normal]
    msg_urgency = normal
    background = "#282c34"
    foreground = "#abb2bf"

[base16_critical]
    msg_urgency = critical
    background = "#e06c75"
    foreground = "#3e4451"
