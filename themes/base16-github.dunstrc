[global]
    frame_color = "#333333"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#795da3"
    foreground = "#c8c8fa"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#333333"

[base16_critical]
    msg_urgency = critical
    background = "#ed6a43"
    foreground = "#c8c8fa"
