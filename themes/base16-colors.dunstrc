# Base16 Colors
# Scheme author: mrmrs (http://clrs.cc)

[global]
    frame_color = "#bbbbbb"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0074d9"
    foreground = "#555555"

[base16_normal]
    msg_urgency = normal
    background = "#111111"
    foreground = "#bbbbbb"

[base16_critical]
    msg_urgency = critical
    background = "#ff4136"
    foreground = "#555555"
