[global]
    frame_color = "#343b59"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#34548a"
    foreground = "#dfe0e5"

[base16_normal]
    msg_urgency = normal
    background = "#d5d6db"
    foreground = "#343b59"

[base16_critical]
    msg_urgency = critical
    background = "#343b58"
    foreground = "#dfe0e5"
