# Base16 Nord
# Scheme author: arcticicestudio

[global]
    frame_color = "#e5e9f0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#81a1c1"
    foreground = "#434c5e"

[base16_normal]
    msg_urgency = normal
    background = "#2e3440"
    foreground = "#e5e9f0"

[base16_critical]
    msg_urgency = critical
    background = "#bf616a"
    foreground = "#434c5e"
