# Base16 lime
# Scheme author: limelier

[global]
    frame_color = "#818175"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#2b926f"
    foreground = "#2a2a3f"

[base16_normal]
    msg_urgency = normal
    background = "#1a1a2f"
    foreground = "#818175"

[base16_critical]
    msg_urgency = critical
    background = "#ff662a"
    foreground = "#2a2a3f"
