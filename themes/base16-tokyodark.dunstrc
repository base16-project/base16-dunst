# Base16 Tokyodark
# Scheme author: Tiagovla (https://github.com/tiagovla/)

[global]
    frame_color = "#abb2bf"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#95c561"
    foreground = "#43444f"

[base16_normal]
    msg_urgency = normal
    background = "#11121d"
    foreground = "#abb2bf"

[base16_critical]
    msg_urgency = critical
    background = "#a485dd"
    foreground = "#43444f"
