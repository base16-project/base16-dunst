[global]
    frame_color = "#343b59"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#34548a"
    foreground = "#edeff6"

[base16_normal]
    msg_urgency = normal
    background = "#fbfbfd"
    foreground = "#343b59"

[base16_critical]
    msg_urgency = critical
    background = "#8c4351"
    foreground = "#edeff6"
