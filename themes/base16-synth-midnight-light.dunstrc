# Base16 Synth Midnight Terminal Light
# Scheme author: MichaÃ«l Ball (http://github.com/michael-ball/)

[global]
    frame_color = "#28292a"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#03aeff"
    foreground = "#c1c3c4"

[base16_normal]
    msg_urgency = normal
    background = "#dddfe0"
    foreground = "#28292a"

[base16_critical]
    msg_urgency = critical
    background = "#b53b50"
    foreground = "#c1c3c4"
