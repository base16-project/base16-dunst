# Base16 Pico
# Scheme author: PICO-8 (http://www.lexaloffle.com/pico-8.php)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#5f574f"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#83769c"
    foreground = "#7e2553"

[base16_normal]
    msg_urgency = normal
    background = "#000000"
    foreground = "#5f574f"

[base16_critical]
    msg_urgency = critical
    background = "#ff004d"
    foreground = "#7e2553"
