# Base16 Catppuccin
# Scheme author: Pocco81 (https://github.com/pocco81)

[global]
    frame_color = "#d7dae0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#96cdfb"
    foreground = "#302d41"

[base16_normal]
    msg_urgency = normal
    background = "#1e1e28"
    foreground = "#d7dae0"

[base16_critical]
    msg_urgency = critical
    background = "#f28fad"
    foreground = "#302d41"
