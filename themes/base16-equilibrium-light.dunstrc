# Base16 Equilibrium Light
# Scheme author: Carlo Abelli

[global]
    frame_color = "#43474e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0073b5"
    foreground = "#d8d4cb"

[base16_normal]
    msg_urgency = normal
    background = "#f5f0e7"
    foreground = "#43474e"

[base16_critical]
    msg_urgency = critical
    background = "#d02023"
    foreground = "#d8d4cb"
