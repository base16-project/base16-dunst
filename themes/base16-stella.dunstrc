# Base16 Stella
# Scheme author: Shrimpram

[global]
    frame_color = "#998bad"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#a5aad4"
    foreground = "#4d4160"

[base16_normal]
    msg_urgency = normal
    background = "#2b213c"
    foreground = "#998bad"

[base16_critical]
    msg_urgency = critical
    background = "#c79987"
    foreground = "#4d4160"
