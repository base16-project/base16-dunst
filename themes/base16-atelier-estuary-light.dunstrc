# Base16 Atelier Estuary Light
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#5f5e4e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#36a166"
    foreground = "#929181"

[base16_normal]
    msg_urgency = normal
    background = "#f4f3ec"
    foreground = "#5f5e4e"

[base16_critical]
    msg_urgency = critical
    background = "#ba6236"
    foreground = "#929181"
