# Base16 Da One Ocean
# Scheme author: NNB (https://github.com/NNBnh)

[global]
    frame_color = "#ffffff"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6bb8ff"
    foreground = "#525866"

[base16_normal]
    msg_urgency = normal
    background = "#171726"
    foreground = "#ffffff"

[base16_critical]
    msg_urgency = critical
    background = "#fa7883"
    foreground = "#525866"
