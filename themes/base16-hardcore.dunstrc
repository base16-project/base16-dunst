# Base16 Hardcore
# Scheme author: Chris Caller

[global]
    frame_color = "#cdcdcd"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#66d9ef"
    foreground = "#353535"

[base16_normal]
    msg_urgency = normal
    background = "#212121"
    foreground = "#cdcdcd"

[base16_critical]
    msg_urgency = critical
    background = "#f92672"
    foreground = "#353535"
