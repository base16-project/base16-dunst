# Base16 Hopscotch
# Scheme author: Jan T. Sott

[global]
    frame_color = "#b9b5b8"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#1290bf"
    foreground = "#5c545b"

[base16_normal]
    msg_urgency = normal
    background = "#322931"
    foreground = "#b9b5b8"

[base16_critical]
    msg_urgency = critical
    background = "#dd464c"
    foreground = "#5c545b"
