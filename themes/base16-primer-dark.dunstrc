# Base16 Primer Dark
# Scheme author: Jimmy Lin

[global]
    frame_color = "#b1bac4"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#58a6ff"
    foreground = "#30363d"

[base16_normal]
    msg_urgency = normal
    background = "#010409"
    foreground = "#b1bac4"

[base16_critical]
    msg_urgency = critical
    background = "#ff7b72"
    foreground = "#30363d"
