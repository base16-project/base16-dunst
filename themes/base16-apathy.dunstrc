# Base16 Apathy
# Scheme author: Jannik Siebert (https://github.com/janniks)

[global]
    frame_color = "#81b5ac"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#96883e"
    foreground = "#184e45"

[base16_normal]
    msg_urgency = normal
    background = "#031a16"
    foreground = "#81b5ac"

[base16_critical]
    msg_urgency = critical
    background = "#3e9688"
    foreground = "#184e45"
