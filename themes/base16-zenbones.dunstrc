# Base16 Zenbones
# Scheme author: mcchrish

[global]
    frame_color = "#b279a7"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#cf86c1"
    foreground = "#819b69"

[base16_normal]
    msg_urgency = normal
    background = "#191919"
    foreground = "#b279a7"

[base16_critical]
    msg_urgency = critical
    background = "#3d3839"
    foreground = "#819b69"
