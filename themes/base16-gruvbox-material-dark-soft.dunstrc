# Base16 Gruvbox Material Dark, Soft
# Scheme author: Mayush Kumar (https://github.com/MayushKumar), sainnhe (https://github.com/sainnhe/gruvbox-material-vscode)

[global]
    frame_color = "#ddc7a1"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7daea3"
    foreground = "#5a524c"

[base16_normal]
    msg_urgency = normal
    background = "#32302f"
    foreground = "#ddc7a1"

[base16_critical]
    msg_urgency = critical
    background = "#ea6962"
    foreground = "#5a524c"
