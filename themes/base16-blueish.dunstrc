# Base16 Blueish
# Scheme author: Ben Mayoras
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#c8e1f8"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#82aaff"
    foreground = "#46290a"

[base16_normal]
    msg_urgency = normal
    background = "#182430"
    foreground = "#c8e1f8"

[base16_critical]
    msg_urgency = critical
    background = "#4ce587"
    foreground = "#46290a"
