# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#4c505e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#34548a"
    foreground = "#dfe0e5"

[base16_normal]
    msg_urgency = normal
    background = "#d5d6db"
    foreground = "#4c505e"

[base16_critical]
    msg_urgency = critical
    background = "#8c4351"
    foreground = "#dfe0e5"
