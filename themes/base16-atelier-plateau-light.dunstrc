# Base16 Atelier Plateau Light
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#585050"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7272ca"
    foreground = "#8a8585"

[base16_normal]
    msg_urgency = normal
    background = "#f4ecec"
    foreground = "#585050"

[base16_critical]
    msg_urgency = critical
    background = "#ca4949"
    foreground = "#8a8585"
