# Base16 Zenburn
# Scheme author: elnawe

[global]
    frame_color = "#dcdccc"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7cb8bb"
    foreground = "#606060"

[base16_normal]
    msg_urgency = normal
    background = "#383838"
    foreground = "#dcdccc"

[base16_critical]
    msg_urgency = critical
    background = "#dca3a3"
    foreground = "#606060"
