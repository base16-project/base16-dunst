# Scheme author: zoe firi (zoefiri.github.io)

[global]
    frame_color = "#736e55"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#489bf0"
    foreground = "#3a3527"

[base16_normal]
    msg_urgency = normal
    background = "#1c1810"
    foreground = "#736e55"

[base16_critical]
    msg_urgency = critical
    background = "#e35142"
    foreground = "#3a3527"
