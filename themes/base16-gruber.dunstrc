# Base16 Gruber
# Scheme author: Patel, Nimai &lt;nimai.m.patel@gmail.com&gt;, colors from www.github.com/rexim/gruber-darker-theme

[global]
    frame_color = "#f4f4ff"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#96a6c8"
    foreground = "#484848"

[base16_normal]
    msg_urgency = normal
    background = "#181818"
    foreground = "#f4f4ff"

[base16_critical]
    msg_urgency = critical
    background = "#f43841"
    foreground = "#484848"
