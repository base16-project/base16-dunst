# Base16 Icy Dark
# Scheme author: icyphox (https://icyphox.ga)

[global]
    frame_color = "#095b67"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#00bcd4"
    foreground = "#041f23"

[base16_normal]
    msg_urgency = normal
    background = "#021012"
    foreground = "#095b67"

[base16_critical]
    msg_urgency = critical
    background = "#16c1d9"
    foreground = "#041f23"
