# Base16 Material Palenight
# Scheme author: Nate Peterson

[global]
    frame_color = "#959dcb"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#82aaff"
    foreground = "#32374d"

[base16_normal]
    msg_urgency = normal
    background = "#292d3e"
    foreground = "#959dcb"

[base16_critical]
    msg_urgency = critical
    background = "#f07178"
    foreground = "#32374d"
