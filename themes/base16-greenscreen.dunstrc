# Base16 Green Screen
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#00bb00"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#009900"
    foreground = "#005500"

[base16_normal]
    msg_urgency = normal
    background = "#001100"
    foreground = "#00bb00"

[base16_critical]
    msg_urgency = critical
    background = "#007700"
    foreground = "#005500"
