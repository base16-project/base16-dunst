# Base16 Primer Light
# Scheme author: Jimmy Lin

[global]
    frame_color = "#2f363d"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0366d6"
    foreground = "#d1d5da"

[base16_normal]
    msg_urgency = normal
    background = "#fafbfc"
    foreground = "#2f363d"

[base16_critical]
    msg_urgency = critical
    background = "#d73a49"
    foreground = "#d1d5da"
