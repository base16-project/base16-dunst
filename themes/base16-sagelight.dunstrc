# Base16 Sagelight
# Scheme author: Carter Veldhuizen

[global]
    frame_color = "#383838"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#a0a7d2"
    foreground = "#d8d8d8"

[base16_normal]
    msg_urgency = normal
    background = "#f8f8f8"
    foreground = "#383838"

[base16_critical]
    msg_urgency = critical
    background = "#fa8480"
    foreground = "#d8d8d8"
