# Base16 Katy
# Scheme author: George Essig (https://github.com/gessig)

[global]
    frame_color = "#959dcb"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#82aaff"
    foreground = "#5c598b"

[base16_normal]
    msg_urgency = normal
    background = "#292d3e"
    foreground = "#959dcb"

[base16_critical]
    msg_urgency = critical
    background = "#6e98e1"
    foreground = "#5c598b"
