# Base16 Edge Dark
# Scheme author: cjayross (https://github.com/cjayross)

[global]
    frame_color = "#b7bec9"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#73b3e7"
    foreground = "#b7bec9"

[base16_normal]
    msg_urgency = normal
    background = "#262729"
    foreground = "#b7bec9"

[base16_critical]
    msg_urgency = critical
    background = "#e77171"
    foreground = "#b7bec9"
