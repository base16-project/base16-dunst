[global]
    frame_color = "#5f5f87"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#8787af"
    foreground = "#5f875f"

[base16_normal]
    msg_urgency = normal
    background = "#262626"
    foreground = "#5f5f87"

[base16_critical]
    msg_urgency = critical
    background = "#444444"
    foreground = "#5f875f"
