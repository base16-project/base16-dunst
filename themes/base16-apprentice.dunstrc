[global]
    frame_color = "#bcbcbc"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#ffffaf"
    foreground = "#333333"

[base16_normal]
    msg_urgency = normal
    background = "#262626"
    foreground = "#bcbcbc"

[base16_critical]
    msg_urgency = critical
    background = "#5f8787"
    foreground = "#333333"
