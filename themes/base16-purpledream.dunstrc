# Base16 Purpledream
# Scheme author: malet

[global]
    frame_color = "#ddd0dd"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#00a0f0"
    foreground = "#403040"

[base16_normal]
    msg_urgency = normal
    background = "#100510"
    foreground = "#ddd0dd"

[base16_critical]
    msg_urgency = critical
    background = "#ff1d0d"
    foreground = "#403040"
