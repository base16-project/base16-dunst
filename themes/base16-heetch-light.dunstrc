# Base16 Heetch Light
# Scheme author: Geoffrey Teale (tealeg@gmail.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#5a496e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#47f9f5"
    foreground = "#7b6d8b"

[base16_normal]
    msg_urgency = normal
    background = "#feffff"
    foreground = "#5a496e"

[base16_critical]
    msg_urgency = critical
    background = "#27d9d5"
    foreground = "#7b6d8b"
