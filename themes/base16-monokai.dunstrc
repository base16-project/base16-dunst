# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#f8f8f2"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#66d9ef"
    foreground = "#49483e"

[base16_normal]
    msg_urgency = normal
    background = "#272822"
    foreground = "#f8f8f2"

[base16_critical]
    msg_urgency = critical
    background = "#f92672"
    foreground = "#49483e"
