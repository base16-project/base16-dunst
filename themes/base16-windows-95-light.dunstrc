# Base16 Windows 95 Light
# Scheme author: Fergus Collins (https://github.com/C-Fergus)

[global]
    frame_color = "#545454"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0000a8"
    foreground = "#c4c4c4"

[base16_normal]
    msg_urgency = normal
    background = "#fcfcfc"
    foreground = "#545454"

[base16_critical]
    msg_urgency = critical
    background = "#a80000"
    foreground = "#c4c4c4"
