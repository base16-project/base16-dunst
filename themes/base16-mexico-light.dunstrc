# Base16 Mexico Light
# Scheme author: Sheldon Johnson
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#383838"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7cafc2"
    foreground = "#d8d8d8"

[base16_normal]
    msg_urgency = normal
    background = "#f8f8f8"
    foreground = "#383838"

[base16_critical]
    msg_urgency = critical
    background = "#ab4642"
    foreground = "#d8d8d8"
