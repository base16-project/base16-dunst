[global]
    frame_color = "#cdd3de"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#89ddff"
    foreground = "#37474f"

[base16_normal]
    msg_urgency = normal
    background = "#263238"
    foreground = "#cdd3de"

[base16_critical]
    msg_urgency = critical
    background = "#ec5f67"
    foreground = "#37474f"
