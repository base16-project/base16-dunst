# Base16 Atelier Savanna
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#87928a"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#478c90"
    foreground = "#526057"

[base16_normal]
    msg_urgency = normal
    background = "#171c19"
    foreground = "#87928a"

[base16_critical]
    msg_urgency = critical
    background = "#b16139"
    foreground = "#526057"
