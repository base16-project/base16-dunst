# Base16 Embers
# Scheme author: Jannik Siebert (https://github.com/janniks)

[global]
    frame_color = "#a39a90"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6d5782"
    foreground = "#433b32"

[base16_normal]
    msg_urgency = normal
    background = "#16130f"
    foreground = "#a39a90"

[base16_critical]
    msg_urgency = critical
    background = "#826d57"
    foreground = "#433b32"
