# Base16 Brush Trees
# Scheme author: Abraham White &lt;abelincoln.white@gmail.com&gt;

[global]
    frame_color = "#6d828e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#868cb3"
    foreground = "#b0c5c8"

[base16_normal]
    msg_urgency = normal
    background = "#e3efef"
    foreground = "#6d828e"

[base16_critical]
    msg_urgency = critical
    background = "#b38686"
    foreground = "#b0c5c8"
