# Base16 Equilibrium Gray Light
# Scheme author: Carlo Abelli

[global]
    frame_color = "#474747"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0073b5"
    foreground = "#d4d4d4"

[base16_normal]
    msg_urgency = normal
    background = "#f1f1f1"
    foreground = "#474747"

[base16_critical]
    msg_urgency = critical
    background = "#d02023"
    foreground = "#d4d4d4"
