# Base16 Unikitty Dark
# Scheme author: Josh W Lewis (@joshwlewis)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#bcbabe"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#796af5"
    foreground = "#666369"

[base16_normal]
    msg_urgency = normal
    background = "#2e2a31"
    foreground = "#bcbabe"

[base16_critical]
    msg_urgency = critical
    background = "#d8137f"
    foreground = "#666369"
