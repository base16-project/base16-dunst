# Base16 ShadeSmear Dark
# Scheme author: Kyle Giammarco (http://kyle.giammar.co)

[global]
    frame_color = "#dbdbdb"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#376388"
    foreground = "#4e4e4e"

[base16_normal]
    msg_urgency = normal
    background = "#232323"
    foreground = "#dbdbdb"

[base16_critical]
    msg_urgency = critical
    background = "#cc5450"
    foreground = "#4e4e4e"
