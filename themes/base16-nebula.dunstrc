# Base16 Nebula
# Scheme author: Gabriel Fontes (https://github.com/Misterio77)

[global]
    frame_color = "#a4a6a9"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#4d6bb6"
    foreground = "#5a8380"

[base16_normal]
    msg_urgency = normal
    background = "#22273b"
    foreground = "#a4a6a9"

[base16_critical]
    msg_urgency = critical
    background = "#777abc"
    foreground = "#5a8380"
