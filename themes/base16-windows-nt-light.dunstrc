# Base16 Windows NT Light
# Scheme author: Fergus Collins (https://github.com/C-Fergus)

[global]
    frame_color = "#808080"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#000080"
    foreground = "#d5d5d5"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#808080"

[base16_critical]
    msg_urgency = critical
    background = "#800000"
    foreground = "#d5d5d5"
