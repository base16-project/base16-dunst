# Base16 Material Lighter
# Scheme author: Nate Peterson

[global]
    frame_color = "#80cbc4"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6182b8"
    foreground = "#cceae7"

[base16_normal]
    msg_urgency = normal
    background = "#fafafa"
    foreground = "#80cbc4"

[base16_critical]
    msg_urgency = critical
    background = "#ff5370"
    foreground = "#cceae7"
