# Base16 Isotope
# Scheme author: Jan T. Sott
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#d0d0d0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0066ff"
    foreground = "#606060"

[base16_normal]
    msg_urgency = normal
    background = "#000000"
    foreground = "#d0d0d0"

[base16_critical]
    msg_urgency = critical
    background = "#ff0000"
    foreground = "#606060"
