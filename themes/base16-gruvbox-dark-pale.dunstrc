# Base16 Gruvbox dark, pale
# Scheme author: Dawid Kurek (dawikur@gmail.com), morhetz (https://github.com/morhetz/gruvbox)

[global]
    frame_color = "#dab997"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#83adad"
    foreground = "#4e4e4e"

[base16_normal]
    msg_urgency = normal
    background = "#262626"
    foreground = "#dab997"

[base16_critical]
    msg_urgency = critical
    background = "#d75f5f"
    foreground = "#4e4e4e"
