[global]
    frame_color = "#5b778c"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#977d7c"
    foreground = "#003552"

[base16_normal]
    msg_urgency = normal
    background = "#041523"
    foreground = "#5b778c"

[base16_critical]
    msg_urgency = critical
    background = "#818591"
    foreground = "#003552"
