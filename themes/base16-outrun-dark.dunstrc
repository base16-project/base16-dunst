# Base16 Outrun Dark
# Scheme author: Hugo Delahousse (http://github.com/hugodelahousse/)

[global]
    frame_color = "#d0d0fa"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#66b0ff"
    foreground = "#30305a"

[base16_normal]
    msg_urgency = normal
    background = "#00002a"
    foreground = "#d0d0fa"

[base16_critical]
    msg_urgency = critical
    background = "#ff4242"
    foreground = "#30305a"
