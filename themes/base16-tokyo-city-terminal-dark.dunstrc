[global]
    frame_color = "#d8e2ec"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#539afc"
    foreground = "#28323a"

[base16_normal]
    msg_urgency = normal
    background = "#171d23"
    foreground = "#d8e2ec"

[base16_critical]
    msg_urgency = critical
    background = "#d95468"
    foreground = "#28323a"
