# Base16 Horizon Terminal Light
# Scheme author: MichaÃ«l Ball (http://github.com/michael-ball/)

[global]
    frame_color = "#403c3d"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#26bbd9"
    foreground = "#f9cbbe"

[base16_normal]
    msg_urgency = normal
    background = "#fdf0ed"
    foreground = "#403c3d"

[base16_critical]
    msg_urgency = critical
    background = "#e95678"
    foreground = "#f9cbbe"
