# Base16 Default Dark
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#d8d8d8"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7cafc2"
    foreground = "#383838"

[base16_normal]
    msg_urgency = normal
    background = "#181818"
    foreground = "#d8d8d8"

[base16_critical]
    msg_urgency = critical
    background = "#ab4642"
    foreground = "#383838"
