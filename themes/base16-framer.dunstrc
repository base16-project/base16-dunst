# Base16 Framer
# Scheme author: Framer (Maintained by Jesse Hoyos)

[global]
    frame_color = "#d0d0d0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#20bcfc"
    foreground = "#464646"

[base16_normal]
    msg_urgency = normal
    background = "#181818"
    foreground = "#d0d0d0"

[base16_critical]
    msg_urgency = critical
    background = "#fd886b"
    foreground = "#464646"
