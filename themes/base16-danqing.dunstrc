# Base16 DanQing
# Scheme author: Wenhan Zhu (Cosmos) (zhuwenhan950913@gmail.com)

[global]
    frame_color = "#e0f0ef"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#b0a4e3"
    foreground = "#5a605d"

[base16_normal]
    msg_urgency = normal
    background = "#2d302f"
    foreground = "#e0f0ef"

[base16_critical]
    msg_urgency = critical
    background = "#f9906f"
    foreground = "#5a605d"
