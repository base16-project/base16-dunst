# Base16 Twilight
# Scheme author: David Hart (https://github.com/hartbit)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#a7a7a7"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7587a6"
    foreground = "#464b50"

[base16_normal]
    msg_urgency = normal
    background = "#1e1e1e"
    foreground = "#a7a7a7"

[base16_critical]
    msg_urgency = critical
    background = "#cf6a4c"
    foreground = "#464b50"
