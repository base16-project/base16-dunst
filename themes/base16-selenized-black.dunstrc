# Base16 selenized-black
# Scheme author: Jan Warchol (https://github.com/jan-warchol/selenized) / adapted to base16 by ali

[global]
    frame_color = "#b9b9b9"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#368aeb"
    foreground = "#3b3b3b"

[base16_normal]
    msg_urgency = normal
    background = "#181818"
    foreground = "#b9b9b9"

[base16_critical]
    msg_urgency = critical
    background = "#ed4a46"
    foreground = "#3b3b3b"
