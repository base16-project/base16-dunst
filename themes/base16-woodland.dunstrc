# Base16 Woodland
# Scheme author: Jay Cornwall (https://jcornwall.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#cabcb1"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#88a4d3"
    foreground = "#48413a"

[base16_normal]
    msg_urgency = normal
    background = "#231e18"
    foreground = "#cabcb1"

[base16_critical]
    msg_urgency = critical
    background = "#d35c5c"
    foreground = "#48413a"
