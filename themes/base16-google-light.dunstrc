# Base16 Google Light
# Scheme author: Seth Wright (http://sethawright.com)

[global]
    frame_color = "#373b41"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#3971ed"
    foreground = "#c5c8c6"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#373b41"

[base16_critical]
    msg_urgency = critical
    background = "#cc342b"
    foreground = "#c5c8c6"
