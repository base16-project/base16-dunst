[global]
    frame_color = "#9ea7a6"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#484d79"
    foreground = "#2a343a"

[base16_normal]
    msg_urgency = normal
    background = "#232c31"
    foreground = "#9ea7a6"

[base16_critical]
    msg_urgency = critical
    background = "#2a5491"
    foreground = "#2a343a"
