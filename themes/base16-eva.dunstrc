# Base16 Eva
# Scheme author: kjakapat (https://github.com/kjakapat)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#9fa2a6"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#15f4ee"
    foreground = "#4b6988"

[base16_normal]
    msg_urgency = normal
    background = "#2a3b4d"
    foreground = "#9fa2a6"

[base16_critical]
    msg_urgency = critical
    background = "#c4676c"
    foreground = "#4b6988"
