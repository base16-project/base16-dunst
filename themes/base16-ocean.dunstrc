# Base16 Ocean
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#c0c5ce"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#8fa1b3"
    foreground = "#4f5b66"

[base16_normal]
    msg_urgency = normal
    background = "#2b303b"
    foreground = "#c0c5ce"

[base16_critical]
    msg_urgency = critical
    background = "#bf616a"
    foreground = "#4f5b66"
