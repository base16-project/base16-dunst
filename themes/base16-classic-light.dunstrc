# Base16 Classic Light
# Scheme author: Jason Heeris (http://heeris.id.au)

[global]
    frame_color = "#303030"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6a9fb5"
    foreground = "#d0d0d0"

[base16_normal]
    msg_urgency = normal
    background = "#f5f5f5"
    foreground = "#303030"

[base16_critical]
    msg_urgency = critical
    background = "#ac4142"
    foreground = "#d0d0d0"
