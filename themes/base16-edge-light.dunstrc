# Base16 Edge Light
# Scheme author: cjayross (https://github.com/cjayross)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#5e646f"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6587bf"
    foreground = "#d69822"

[base16_normal]
    msg_urgency = normal
    background = "#fafafa"
    foreground = "#5e646f"

[base16_critical]
    msg_urgency = critical
    background = "#db7070"
    foreground = "#d69822"
