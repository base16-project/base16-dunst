# Base16 Solarized Light
# Scheme author: Ethan Schoonover (modified by aramisgithub)

[global]
    frame_color = "#586e75"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#268bd2"
    foreground = "#93a1a1"

[base16_normal]
    msg_urgency = normal
    background = "#fdf6e3"
    foreground = "#586e75"

[base16_critical]
    msg_urgency = critical
    background = "#dc322f"
    foreground = "#93a1a1"
