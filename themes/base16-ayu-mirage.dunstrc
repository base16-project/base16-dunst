[global]
    frame_color = "#cccac2"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#5ccfe6"
    foreground = "#242936"

[base16_normal]
    msg_urgency = normal
    background = "#171b24"
    foreground = "#cccac2"

[base16_critical]
    msg_urgency = critical
    background = "#f28779"
    foreground = "#242936"
