# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#ffffff"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6bb8ff"
    foreground = "#525866"

[base16_normal]
    msg_urgency = normal
    background = "#22273d"
    foreground = "#ffffff"

[base16_critical]
    msg_urgency = critical
    background = "#fa7883"
    foreground = "#525866"
