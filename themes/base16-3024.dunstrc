[global]
    frame_color = "#a5a2a2"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#01a0e4"
    foreground = "#4a4543"

[base16_normal]
    msg_urgency = normal
    background = "#090300"
    foreground = "#a5a2a2"

[base16_critical]
    msg_urgency = critical
    background = "#db2d20"
    foreground = "#4a4543"
