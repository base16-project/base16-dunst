# Base16 Cupertino
# Scheme author: Defman21

[global]
    frame_color = "#404040"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0000ff"
    foreground = "#c0c0c0"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#404040"

[base16_critical]
    msg_urgency = critical
    background = "#c41a15"
    foreground = "#c0c0c0"
