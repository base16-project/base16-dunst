# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#8b8792"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#576ddb"
    foreground = "#585260"

[base16_normal]
    msg_urgency = normal
    background = "#19171c"
    foreground = "#8b8792"

[base16_critical]
    msg_urgency = critical
    background = "#be4678"
    foreground = "#585260"
