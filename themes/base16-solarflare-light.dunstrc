# Base16 Solar Flare Light
# Scheme author: Chuck Harmston (https://chuck.harmston.ch)

[global]
    frame_color = "#586875"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#33b5e1"
    foreground = "#a6afb8"

[base16_normal]
    msg_urgency = normal
    background = "#f5f7fa"
    foreground = "#586875"

[base16_critical]
    msg_urgency = critical
    background = "#ef5253"
    foreground = "#a6afb8"
