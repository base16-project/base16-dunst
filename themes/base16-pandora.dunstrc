# Base16 pandora
# Scheme author: Cassandra Fox
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#f15c99"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#008080"
    foreground = "#472234"

[base16_normal]
    msg_urgency = normal
    background = "#131213"
    foreground = "#f15c99"

[base16_critical]
    msg_urgency = critical
    background = "#b00b69"
    foreground = "#472234"
