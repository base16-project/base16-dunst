[global]
    frame_color = "#a9b1d6"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#2ac3de"
    foreground = "#2f3549"

[base16_normal]
    msg_urgency = normal
    background = "#1a1b26"
    foreground = "#a9b1d6"

[base16_critical]
    msg_urgency = critical
    background = "#c0caf5"
    foreground = "#2f3549"
