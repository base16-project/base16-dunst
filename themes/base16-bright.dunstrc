# Base16 Bright
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#e0e0e0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6fb3d2"
    foreground = "#505050"

[base16_normal]
    msg_urgency = normal
    background = "#000000"
    foreground = "#e0e0e0"

[base16_critical]
    msg_urgency = critical
    background = "#fb0120"
    foreground = "#505050"
