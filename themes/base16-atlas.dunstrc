# Base16 Atlas
# Scheme author: Alex Lende (https://ajlende.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#a1a19a"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#5dd7b9"
    foreground = "#517f8d"

[base16_normal]
    msg_urgency = normal
    background = "#002635"
    foreground = "#a1a19a"

[base16_critical]
    msg_urgency = critical
    background = "#ff5a67"
    foreground = "#517f8d"
