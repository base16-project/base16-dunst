[global]
    frame_color = "#8b9cbe"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#00eaff"
    foreground = "#3c3f4c"

[base16_normal]
    msg_urgency = normal
    background = "#17191e"
    foreground = "#8b9cbe"

[base16_critical]
    msg_urgency = critical
    background = "#ff29a8"
    foreground = "#3c3f4c"
