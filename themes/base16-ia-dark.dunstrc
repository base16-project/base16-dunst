# Base16 iA Dark
# Scheme author: iA Inc. (modified by aramisgithub)

[global]
    frame_color = "#cccccc"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#8eccdd"
    foreground = "#1d414d"

[base16_normal]
    msg_urgency = normal
    background = "#1a1a1a"
    foreground = "#cccccc"

[base16_critical]
    msg_urgency = critical
    background = "#d88568"
    foreground = "#1d414d"
