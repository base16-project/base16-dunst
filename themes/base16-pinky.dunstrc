# Base16 pinky
# Scheme author: Benjamin (https://github.com/b3nj5m1n)

[global]
    frame_color = "#f5f5f5"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#00ffff"
    foreground = "#1d1b1d"

[base16_normal]
    msg_urgency = normal
    background = "#171517"
    foreground = "#f5f5f5"

[base16_critical]
    msg_urgency = critical
    background = "#ffa600"
    foreground = "#1d1b1d"
