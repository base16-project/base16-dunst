# Base16 Humanoid light
# Scheme author: Thomas (tasmo) Friese
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#232629"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0082c9"
    foreground = "#deded8"

[base16_normal]
    msg_urgency = normal
    background = "#f8f8f2"
    foreground = "#232629"

[base16_critical]
    msg_urgency = critical
    background = "#b0151a"
    foreground = "#deded8"
