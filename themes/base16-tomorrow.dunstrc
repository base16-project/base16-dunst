# Base16 Tomorrow
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#4d4d4c"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#4271ae"
    foreground = "#d6d6d6"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#4d4d4c"

[base16_critical]
    msg_urgency = critical
    background = "#c82829"
    foreground = "#d6d6d6"
