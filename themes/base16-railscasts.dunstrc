# Base16 Railscasts
# Scheme author: Ryan Bates (http://railscasts.com)

[global]
    frame_color = "#e6e1dc"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6d9cbe"
    foreground = "#3a4055"

[base16_normal]
    msg_urgency = normal
    background = "#2b2b2b"
    foreground = "#e6e1dc"

[base16_critical]
    msg_urgency = critical
    background = "#da4939"
    foreground = "#3a4055"
