# Base16 Eighties
# Scheme author: Chris Kempson (http://chriskempson.com)

[global]
    frame_color = "#d3d0c8"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#6699cc"
    foreground = "#515151"

[base16_normal]
    msg_urgency = normal
    background = "#2d2d2d"
    foreground = "#d3d0c8"

[base16_critical]
    msg_urgency = critical
    background = "#f2777a"
    foreground = "#515151"
