# Base16 Helios
# Scheme author: Alex Meyer (https://github.com/reyemxela)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#d5d5d5"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#1e8bac"
    foreground = "#53585b"

[base16_normal]
    msg_urgency = normal
    background = "#1d2021"
    foreground = "#d5d5d5"

[base16_critical]
    msg_urgency = critical
    background = "#d72638"
    foreground = "#53585b"
