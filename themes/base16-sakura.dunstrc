# Base16 Sakura
# Scheme author: Misterio77 (http://github.com/Misterio77)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#564448"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#006e93"
    foreground = "#e0ccd1"

[base16_normal]
    msg_urgency = normal
    background = "#feedf3"
    foreground = "#564448"

[base16_critical]
    msg_urgency = critical
    background = "#df2d52"
    foreground = "#e0ccd1"
