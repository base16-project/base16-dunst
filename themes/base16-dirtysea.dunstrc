# Base16 dirtysea
# Scheme author: Kahlil (Kal) Hodgson
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#000000"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#007300"
    foreground = "#d0d0d0"

[base16_normal]
    msg_urgency = normal
    background = "#e0e0e0"
    foreground = "#000000"

[base16_critical]
    msg_urgency = critical
    background = "#840000"
    foreground = "#d0d0d0"
