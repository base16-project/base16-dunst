# Base16 Tokyodark Terminal
# Scheme author: Tiagovla (https://github.com/tiagovla/)

[global]
    frame_color = "#a0a8cd"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7199ee"
    foreground = "#212234"

[base16_normal]
    msg_urgency = normal
    background = "#11121d"
    foreground = "#a0a8cd"

[base16_critical]
    msg_urgency = critical
    background = "#ee6d85"
    foreground = "#212234"
