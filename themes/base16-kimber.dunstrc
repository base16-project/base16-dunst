# Base16 Kimber
# Scheme author: Mishka Nguyen (https://github.com/akhsiM)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#dedee7"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#537c9c"
    foreground = "#555d55"

[base16_normal]
    msg_urgency = normal
    background = "#222222"
    foreground = "#dedee7"

[base16_critical]
    msg_urgency = critical
    background = "#c88c8c"
    foreground = "#555d55"
