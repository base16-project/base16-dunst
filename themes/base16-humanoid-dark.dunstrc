# Base16 Humanoid dark
# Scheme author: Thomas (tasmo) Friese

[global]
    frame_color = "#f8f8f2"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#00a6fb"
    foreground = "#484e54"

[base16_normal]
    msg_urgency = normal
    background = "#232629"
    foreground = "#f8f8f2"

[base16_critical]
    msg_urgency = critical
    background = "#f11235"
    foreground = "#484e54"
