# Base16 Heetch Dark
# Scheme author: Geoffrey Teale (tealeg@gmail.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#bdb6c5"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#bd0152"
    foreground = "#5a496e"

[base16_normal]
    msg_urgency = normal
    background = "#190134"
    foreground = "#bdb6c5"

[base16_critical]
    msg_urgency = critical
    background = "#27d9d5"
    foreground = "#5a496e"
