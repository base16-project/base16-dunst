# Base16 Bespin
# Scheme author: Jan T. Sott

[global]
    frame_color = "#8a8986"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#5ea6ea"
    foreground = "#5e5d5c"

[base16_normal]
    msg_urgency = normal
    background = "#28211c"
    foreground = "#8a8986"

[base16_critical]
    msg_urgency = critical
    background = "#cf6a4c"
    foreground = "#5e5d5c"
