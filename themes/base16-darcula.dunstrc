# Base16 Darcula
# Scheme author: jetbrains

[global]
    frame_color = "#a9b7c6"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#9876aa"
    foreground = "#323232"

[base16_normal]
    msg_urgency = normal
    background = "#2b2b2b"
    foreground = "#a9b7c6"

[base16_critical]
    msg_urgency = critical
    background = "#4eade5"
    foreground = "#323232"
