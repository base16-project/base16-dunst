# Base16 Atelier Forest
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#a8a19f"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#407ee7"
    foreground = "#68615e"

[base16_normal]
    msg_urgency = normal
    background = "#1b1918"
    foreground = "#a8a19f"

[base16_critical]
    msg_urgency = critical
    background = "#f22c40"
    foreground = "#68615e"
