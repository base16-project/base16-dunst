# Base16 Equilibrium Gray Dark
# Scheme author: Carlo Abelli

[global]
    frame_color = "#ababab"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#008dd1"
    foreground = "#262626"

[base16_normal]
    msg_urgency = normal
    background = "#111111"
    foreground = "#ababab"

[base16_critical]
    msg_urgency = critical
    background = "#f04339"
    foreground = "#262626"
