[global]
    frame_color = "#787c99"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7aa2f7"
    foreground = "#343a52"

[base16_normal]
    msg_urgency = normal
    background = "#24283b"
    foreground = "#787c99"

[base16_critical]
    msg_urgency = critical
    background = "#f7768e"
    foreground = "#343a52"
