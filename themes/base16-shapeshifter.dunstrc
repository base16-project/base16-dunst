# Base16 Shapeshifter
# Scheme author: Tyler Benziger (http://tybenz.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#102015"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#3b48e3"
    foreground = "#ababab"

[base16_normal]
    msg_urgency = normal
    background = "#f9f9f9"
    foreground = "#102015"

[base16_critical]
    msg_urgency = critical
    background = "#e92f2f"
    foreground = "#ababab"
