# Base16 Primer Dark Dimmed
# Scheme author: Jimmy Lin

[global]
    frame_color = "#909dab"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#539bf5"
    foreground = "#444c56"

[base16_normal]
    msg_urgency = normal
    background = "#1c2128"
    foreground = "#909dab"

[base16_critical]
    msg_urgency = critical
    background = "#f47067"
    foreground = "#444c56"
