[global]
    frame_color = "#d6d6d6"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#55b5db"
    foreground = "#3b758c"

[base16_normal]
    msg_urgency = normal
    background = "#151718"
    foreground = "#d6d6d6"

[base16_critical]
    msg_urgency = critical
    background = "#cd3f45"
    foreground = "#3b758c"
