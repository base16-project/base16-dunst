# Base16 Unikitty Light
# Scheme author: Josh W Lewis (@joshwlewis)

[global]
    frame_color = "#6c696e"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#775dff"
    foreground = "#c4c3c5"

[base16_normal]
    msg_urgency = normal
    background = "#ffffff"
    foreground = "#6c696e"

[base16_critical]
    msg_urgency = critical
    background = "#d8137f"
    foreground = "#c4c3c5"
