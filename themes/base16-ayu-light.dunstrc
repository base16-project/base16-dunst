# Scheme author: Khue Nguyen &lt;Z5483Y@gmail.com&gt;

[global]
    frame_color = "#5c6773"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#36a3d9"
    foreground = "#f8f9fa"

[base16_normal]
    msg_urgency = normal
    background = "#fafafa"
    foreground = "#5c6773"

[base16_critical]
    msg_urgency = critical
    background = "#f07178"
    foreground = "#f8f9fa"
