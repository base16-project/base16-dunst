# Base16 Macintosh
# Scheme author: Rebecca Bettencourt (http://www.kreativekorp.com)

[global]
    frame_color = "#c0c0c0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#0000d3"
    foreground = "#404040"

[base16_normal]
    msg_urgency = normal
    background = "#000000"
    foreground = "#c0c0c0"

[base16_critical]
    msg_urgency = critical
    background = "#dd0907"
    foreground = "#404040"
