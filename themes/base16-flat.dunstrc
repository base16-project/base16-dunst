# Base16 Flat
# Scheme author: Chris Kempson (http://chriskempson.com)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#e0e0e0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#3498db"
    foreground = "#7f8c8d"

[base16_normal]
    msg_urgency = normal
    background = "#2c3e50"
    foreground = "#e0e0e0"

[base16_critical]
    msg_urgency = critical
    background = "#e74c3c"
    foreground = "#7f8c8d"
