# Base16 Atelier Heath Light
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#695d69"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#516aec"
    foreground = "#ab9bab"

[base16_normal]
    msg_urgency = normal
    background = "#f7f3f7"
    foreground = "#695d69"

[base16_critical]
    msg_urgency = critical
    background = "#ca402b"
    foreground = "#ab9bab"
