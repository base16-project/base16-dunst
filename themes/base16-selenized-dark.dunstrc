# Base16 selenized-dark
# Scheme author: Jan Warchol (https://github.com/jan-warchol/selenized) / adapted to base16 by ali

[global]
    frame_color = "#adbcbc"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#4695f7"
    foreground = "#2d5b69"

[base16_normal]
    msg_urgency = normal
    background = "#103c48"
    foreground = "#adbcbc"

[base16_critical]
    msg_urgency = critical
    background = "#fa5750"
    foreground = "#2d5b69"
