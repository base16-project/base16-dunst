# Base16 Fruit Soda
# Scheme author: jozip

[global]
    frame_color = "#515151"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#2931df"
    foreground = "#d8d5d5"

[base16_normal]
    msg_urgency = normal
    background = "#f1ecf1"
    foreground = "#515151"

[base16_critical]
    msg_urgency = critical
    background = "#fe3e31"
    foreground = "#d8d5d5"
