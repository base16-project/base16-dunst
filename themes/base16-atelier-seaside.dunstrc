# Base16 Atelier Seaside
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#8ca68c"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#3d62f5"
    foreground = "#5e6e5e"

[base16_normal]
    msg_urgency = normal
    background = "#131513"
    foreground = "#8ca68c"

[base16_critical]
    msg_urgency = critical
    background = "#e6193c"
    foreground = "#5e6e5e"
