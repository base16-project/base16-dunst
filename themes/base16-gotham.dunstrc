# Base16 Gotham
# Scheme author: Andrea Leopardi (arranged by Brett Jones)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#599cab"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#195466"
    foreground = "#091f2e"

[base16_normal]
    msg_urgency = normal
    background = "#0c1014"
    foreground = "#599cab"

[base16_critical]
    msg_urgency = critical
    background = "#c23127"
    foreground = "#091f2e"
