# Base16 Qualia
# Scheme author: isaacwhanson
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#c0c0c0"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#50cacd"
    foreground = "#454545"

[base16_normal]
    msg_urgency = normal
    background = "#101010"
    foreground = "#c0c0c0"

[base16_critical]
    msg_urgency = critical
    background = "#efa6a2"
    foreground = "#454545"
