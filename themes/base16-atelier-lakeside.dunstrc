# Base16 Atelier Lakeside
# Scheme author: Bram de Haan (http://atelierbramdehaan.nl)

[global]
    frame_color = "#7ea2b4"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#257fad"
    foreground = "#516d7b"

[base16_normal]
    msg_urgency = normal
    background = "#161b1d"
    foreground = "#7ea2b4"

[base16_critical]
    msg_urgency = critical
    background = "#d22d72"
    foreground = "#516d7b"
