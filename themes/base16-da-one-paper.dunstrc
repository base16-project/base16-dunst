[global]
    frame_color = "#181818"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#5890f8"
    foreground = "#888888"

[base16_normal]
    msg_urgency = normal
    background = "#faf0dc"
    foreground = "#181818"

[base16_critical]
    msg_urgency = critical
    background = "#de5d6e"
    foreground = "#888888"
