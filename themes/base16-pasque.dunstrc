# Base16 Pasque
# Scheme author: Gabriel Fontes (https://github.com/Misterio77)

[global]
    frame_color = "#dedcdf"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#8e7dc6"
    foreground = "#3e2d5c"

[base16_normal]
    msg_urgency = normal
    background = "#271c3a"
    foreground = "#dedcdf"

[base16_critical]
    msg_urgency = critical
    background = "#a92258"
    foreground = "#3e2d5c"
