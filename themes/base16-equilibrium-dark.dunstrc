# Base16 Equilibrium Dark
# Scheme author: Carlo Abelli
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#afaba2"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#008dd1"
    foreground = "#22262d"

[base16_normal]
    msg_urgency = normal
    background = "#0c1118"
    foreground = "#afaba2"

[base16_critical]
    msg_urgency = critical
    background = "#f04339"
    foreground = "#22262d"
