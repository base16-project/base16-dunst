# Base16 Unikitty Reversible
# Scheme author: Josh W Lewis (@joshwlewis)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#c3c2c4"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#7864fa"
    foreground = "#69666b"

[base16_normal]
    msg_urgency = normal
    background = "#2e2a31"
    foreground = "#c3c2c4"

[base16_critical]
    msg_urgency = critical
    background = "#d8137f"
    foreground = "#69666b"
