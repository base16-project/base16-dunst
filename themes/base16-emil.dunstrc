# Base16 emil
# Scheme author: limelier
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#313145"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#471397"
    foreground = "#9e9eaf"

[base16_normal]
    msg_urgency = normal
    background = "#efefef"
    foreground = "#313145"

[base16_critical]
    msg_urgency = critical
    background = "#f43979"
    foreground = "#9e9eaf"
