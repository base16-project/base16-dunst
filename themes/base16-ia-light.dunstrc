# Base16 iA Light
# Scheme author: iA Inc. (modified by aramisgithub)
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#181818"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#48bac2"
    foreground = "#bde5f2"

[base16_normal]
    msg_urgency = normal
    background = "#f6f6f6"
    foreground = "#181818"

[base16_critical]
    msg_urgency = critical
    background = "#9c5a02"
    foreground = "#bde5f2"
