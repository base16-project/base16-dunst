# Base16 Material Vivid
# Scheme author: joshyrobot
# Template author: Tinted Theming (https://github.com/tinted-theming)

[global]
    frame_color = "#80868b"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#2196f3"
    foreground = "#323639"

[base16_normal]
    msg_urgency = normal
    background = "#202124"
    foreground = "#80868b"

[base16_critical]
    msg_urgency = critical
    background = "#f44336"
    foreground = "#323639"
