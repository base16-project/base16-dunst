# Base16 Windows 95
# Scheme author: Fergus Collins (https://github.com/C-Fergus)

[global]
    frame_color = "#a8a8a8"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#5454fc"
    foreground = "#383838"

[base16_normal]
    msg_urgency = normal
    background = "#000000"
    foreground = "#a8a8a8"

[base16_critical]
    msg_urgency = critical
    background = "#fc5454"
    foreground = "#383838"
