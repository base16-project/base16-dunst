# Base16 Blue Forest
# Scheme author: alonsodomin (https://github.com/alonsodomin)

[global]
    frame_color = "#ffcc33"
    separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#a2cff5"
    foreground = "#273e5c"

[base16_normal]
    msg_urgency = normal
    background = "#141f2e"
    foreground = "#ffcc33"

[base16_critical]
    msg_urgency = critical
    background = "#fffab1"
    foreground = "#273e5c"
