[global]
frame_color = "#{{base05-hex}}"
separator_color = frame

[base16_low]
    msg_urgency = low
    background = "#{{base0C-hex}}"
    foreground = "#{{base00-hex}}"

[base16_normal]
    msg_urgency = normal
    background = "#{{base00-hex}}"
    foreground = "#{{base05-hex}}"

[base16_critical]
    msg_urgency = critical
    background = "#{{base08-hex}}"
    foreground = "#{{base00-hex}}"
